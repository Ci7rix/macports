#
# Aliases
#

# Homebrew
alias brewc='brew cleanup'
alias brewC='brew cleanup -s'
alias brewd='brew doctor --verbose'
alias brewe='brew edit'
alias brewi='brew info'
alias brewI='brew install'
alias brewl='brew list'
alias brewL='brew leaves'
alias brewo='brew outdated'
alias brewr='brew reinstall'
alias brews='brew search'
alias brewS='brew services'
alias brewu='brew update'
alias brewU='brew upgrade --ignore-pinned'
alias brewx='brew uninstall'
alias brewX='brew uninstall --force'

# Homebrew Cask
alias cask='brew cask'
alias caske='cask edit'
alias caski='cask info'
alias caskI='cask install'
alias caskl='brew list --cask'
alias casko='brew outdated --cask'
alias caskU='brew upgrade --cask'
alias caskx='cask uninstall'
alias caskX='cask uninstall --force'
alias caskz='cask zap'
